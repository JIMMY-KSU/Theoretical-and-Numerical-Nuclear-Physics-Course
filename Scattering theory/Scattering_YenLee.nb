   3.447899446195591*^9, 3.44789945173979*^9}, {3.447980907444526*^9, 
   3.447980917529846*^9}, {3.447981002414542*^9, 3.447981009213849*^9}, {
   3.4493436894803123`*^9, 3.449343690159256*^9}, {3.449346187243*^9, 
   3.449346188079688*^9}, {3.449511056762371*^9, 3.4495110883205957`*^9}, {
   3.4597804212334023`*^9, 3.459780423087714*^9}, {3.459780828015452*^9, 
   3.459780836151882*^9}, {3.4598824938998213`*^9, 3.459882535370378*^9}, {
   3.485176292550352*^9, 3.485176298190042*^9}, {3.485176396716511*^9, 
   3.485176400253709*^9}, {3.48517806739412*^9, 3.485178077424954*^9}, {
