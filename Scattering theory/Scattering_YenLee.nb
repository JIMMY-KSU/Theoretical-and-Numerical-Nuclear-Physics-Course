Cell[TextData[{
 "The idea now is to apply the phase shift to each partial wave, so that \
instead of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["j", "l"], TraditionalForm]],ExpressionUUID->
  "232a79b4-4248-4d8a-a59a-e8e188839422"],
 " we have  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"cos", " ", 
       SubscriptBox["\[Delta]", "l"]}], ")"}], 
     SubscriptBox["j", "l"]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"sin", " ", 
       SubscriptBox["\[Delta]", "l"]}], ")"}], 
     SubscriptBox["y", "l"]}]}], TraditionalForm]],ExpressionUUID->
  "ab3c992f-ca54-4fb4-acc0-57353c6e0d93"],
 "."
}], "Text",
 CellChangeTimes->{{3.449515288847324*^9, 
  3.449515361911429*^9}},ExpressionUUID->"0b584830-adc6-48c0-9b67-\
d485d87c9f09"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"p", "=", "0.01"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"k", "=", "0.2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lmax", "=", "50"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Delta]l", "=", 
   RowBox[{"Table", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ArcTan", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"-", "p"}], " ", 
         RowBox[{"SphericalBesselY", "[", 
          RowBox[{"l", ",", "k"}], "]"}], 
         RowBox[{"jprime", "[", 
          RowBox[{"l", ",", "p"}], "]"}]}], "+", 
        RowBox[{"k", " ", 
         RowBox[{"yprime", "[", 
          RowBox[{"l", ",", "k"}], "]"}], 
         RowBox[{"SphericalBesselJ", "[", 
          RowBox[{"l", ",", "p"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "k"}], " ", 
         RowBox[{"SphericalBesselJ", "[", 
          RowBox[{"l", ",", "p"}], "]"}], 
         RowBox[{"jprime", "[", 
          RowBox[{"l", ",", "k"}], "]"}]}], "+", 
        RowBox[{"p", " ", 
         RowBox[{"jprime", "[", 
          RowBox[{"l", ",", "p"}], "]"}], 
         RowBox[{"SphericalBesselJ", "[", 
          RowBox[{"l", ",", "k"}], "]"}]}]}]}], "\[IndentingNewLine]", "]"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"l", ",", "0", ",", 
       RowBox[{"lmax", "-", "1"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"cl", "=", 
   RowBox[{"Table", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{
         RowBox[{"Cos", "[", "\[Delta]", "]"}], 
         RowBox[{"SphericalBesselJ", "[", 
          RowBox[{"l", ",", "k"}], "]"}]}], "+", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Delta]", "]"}], 
         RowBox[{"SphericalBesselY", "[", 
          RowBox[{"l", ",", "k"}], "]"}]}]}], 
       RowBox[{"SphericalBesselJ", "[", 
        RowBox[{"l", ",", "p"}], "]"}]], "/.", 
      RowBox[{"\[Delta]", "\[Rule]", 
       RowBox[{"\[Delta]l", "\[LeftDoubleBracket]", 
        RowBox[{"1", "+", "l"}], "\[RightDoubleBracket]"}]}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"l", ",", "0", ",", 
       RowBox[{"lmax", "-", "1"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TableForm", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Range", "[", 
      RowBox[{"0", ",", 
       RowBox[{"lmax", "-", "1"}]}], "]"}], ",", "\[Delta]l", ",", "cl"}], 
    "}"}], "]"}], "//", "Chop"}]}], "Input",
