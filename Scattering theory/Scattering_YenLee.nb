Cell[BoxData[
 RowBox[{"\[Sigma]total", "=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"4", "\[Pi]"}], 
     SuperscriptBox["k", "2"]], 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "+", "1"}], ")"}], 
       SuperscriptBox[
        RowBox[{"Sin", "[", 
         RowBox[{"\[Delta]l", "\[LeftDoubleBracket]", 
          RowBox[{"1", "+", "l"}], "\[RightDoubleBracket]"}], "]"}], "2"]}], 
      ",", 
      RowBox[{"{", 
       RowBox[{"l", ",", "0", ",", 
        RowBox[{"lmax", "-", "1"}]}], "}"}]}], "]"}]}], "//", 
   "Chop"}]}]], "Input",
