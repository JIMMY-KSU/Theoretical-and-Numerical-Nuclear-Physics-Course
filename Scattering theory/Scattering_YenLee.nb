Cell[TextData[{
 "It will be convenient to factor as follows: \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{"A", "\[InvisibleSpace]", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "i"}], "\[InvisibleSpace]", "\[Delta]"}]], "\[Times]", 
     RowBox[{"\[Piecewise]", GridBox[{
        {
         RowBox[{"r", "<", "1"}], 
         RowBox[{"b", "\[InvisibleSpace]", 
          RowBox[{"j", "(", 
           RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]}]},
        {
         RowBox[{"r", ">", "1"}], 
         RowBox[{
          RowBox[{
           SuperscriptBox["e", 
            RowBox[{
             RowBox[{"-", "i"}], "\[InvisibleSpace]", "\[Delta]"}]], 
           RowBox[{"h", "(", 
            RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}]}], "+", 
          RowBox[{
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", "\[Delta]"}]], 
           RowBox[{"H", "(", 
            RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}]}]}]}
       }]}]}]}], TraditionalForm]],ExpressionUUID->
  "698eb88d-2478-4932-9b7d-66248e36f53c"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{"A", "\[InvisibleSpace]", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "i"}], "\[InvisibleSpace]", "\[Delta]"}]], "\[Times]", 
     RowBox[{"\[Piecewise]", GridBox[{
        {
         RowBox[{"r", "<", "1"}], 
         RowBox[{"b", "\[InvisibleSpace]", 
          RowBox[{"j", "(", 
           RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]}]},
        {
         RowBox[{"r", ">", "1"}], 
         RowBox[{
          RowBox[{"c", " ", 
           RowBox[{"j", "(", 
            RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}]}], "+", 
          RowBox[{"s", " ", 
           RowBox[{"y", "(", 
            RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}]}]}]}
       }]}]}]}], TraditionalForm]],ExpressionUUID->
  "6f74d10e-ba27-42a5-8dd1-db00dda87bfb"]
