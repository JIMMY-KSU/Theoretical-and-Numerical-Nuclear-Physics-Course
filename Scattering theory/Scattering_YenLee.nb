Cell["Old version", "Subsubsection",
 CellChangeTimes->{{3.4851967968649073`*^9, 
  3.485196798659368*^9}},ExpressionUUID->"caac732d-45ac-4813-bad3-\
7fc6d423bd39"],

Cell[TextData[{
 "Now, for a flat barrier of height ",
 StyleBox["r",
  FontSlant->"Italic"],
 "  and radius 1, let us solve the Schr\[ODoubleDot]dinger equation for an \
incident particle of energy ",
 Cell[BoxData[
  FormBox["E", TraditionalForm]],ExpressionUUID->
  "8229c824-f276-4d32-b965-25b27dec8b8b"],
 ".  The wavenumbers outside and inside the barrier are ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "=", 
    SqrtBox[
     RowBox[{"2", "m", "\[InvisibleSpace]", "E"}]]}], TraditionalForm]],
  ExpressionUUID->"5dcfb7cb-a5b3-400a-83c2-dfcd3da5cac3"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "=", 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"m", "(", 
       RowBox[{"E", "-", 
        SubscriptBox["V", "0"]}], ")"}]}]]}], TraditionalForm]],
  ExpressionUUID->"3eebc968-02f5-4c66-9f3f-78b023cb1a18"],
 ".  The solution is of the form\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{"r", "<", "1"}], 
        RowBox[{
         RowBox[{"A", "\[InvisibleSpace]", 
          RowBox[{"j", "(", 
           RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]}], "+", 
         RowBox[{"B", "\[InvisibleSpace]", 
          RowBox[{"y", "(", 
           RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]}]}]},
       {
        RowBox[{"r", ">", "1"}], 
        RowBox[{
         RowBox[{"C", "\[InvisibleSpace]", 
          RowBox[{"j", "(", 
           RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}]}], "+", 
         RowBox[{"D", "\[InvisibleSpace]", 
          RowBox[{
           RowBox[{"y", "(", 
            RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}], "."}]}]}]}
      }]}]}], TraditionalForm]],ExpressionUUID->
  "5c1244cc-6ad0-480d-ad48-7e91868014f5"],
 "\nIn fact, "
}], "Text",
 CellChangeTimes->{{3.4851968026725893`*^9, 
  3.485196964629506*^9}},ExpressionUUID->"e6bde23e-6600-4a97-8811-\
3c87ffe6ca32"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{"r", "<", "1"}], 
        RowBox[{"j", "(", 
         RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]},
       {
        RowBox[{"r", ">", "1"}], 
        RowBox[{
         RowBox[{"x", "\[InvisibleSpace]", 
          RowBox[{"h", "(", 
           RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}]}], "+", 
         RowBox[{"y", "\[InvisibleSpace]", 
          RowBox[{
           RowBox[{"H", "(", 
            RowBox[{"p", "\[InvisibleSpace]", "r"}], ")"}], "."}]}]}]}
      }]}]}], TraditionalForm]],ExpressionUUID->
  "93f6a26c-8f9b-47a5-b2b7-706f825740b3"],
 "\nNow, for a bound state, the energy is negative, so the free-space \
wavenumber ",
 StyleBox["p",
  FontSlant->"Italic"],
 "  is imaginary: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "=", 
    RowBox[{"i", "\[InvisibleSpace]", "\[Lambda]"}]}], TraditionalForm]],
  ExpressionUUID->"13ff1b70-121c-4d3a-9366-942be3ddde34"],
 ".  Therefore the Hankel functions have imaginary arguments:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{"r", "<", "1"}], 
        RowBox[{"j", "(", 
         RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]},
       {
        RowBox[{"r", ">", "1"}], 
        RowBox[{
         RowBox[{"x", "\[InvisibleSpace]", 
          RowBox[{"h", "(", 
           RowBox[{
           "i", "\[InvisibleSpace]", "\[Lambda]", "\[InvisibleSpace]", "r"}], 
           ")"}]}], "+", 
         RowBox[{"y", "\[InvisibleSpace]", 
          RowBox[{
           RowBox[{"H", "(", 
            RowBox[{
            "i", "\[InvisibleSpace]", "\[Lambda]", "\[InvisibleSpace]", "r"}],
             ")"}], "."}]}]}]}
      }]}]}], TraditionalForm]],ExpressionUUID->
  "33001c46-1af9-4b95-bab2-eddba011969c"],
 "\nOnly H is allowed!  So\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{"r", "<", "1"}], 
        RowBox[{"j", "(", 
         RowBox[{"q", "\[InvisibleSpace]", "r"}], ")"}]},
       {
        RowBox[{"r", ">", "1"}], 
        RowBox[{"y", " ", 
         RowBox[{"H", "(", 
          RowBox[{
          "i", "\[InvisibleSpace]", "\[Lambda]", "\[InvisibleSpace]", "r"}], 
          ")"}]}]}
      }]}]}], TraditionalForm]],ExpressionUUID->
  "ef92c776-5258-4c88-8a42-c33055e77dfb"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "=", 
    SqrtBox[
     RowBox[{
      RowBox[{"-", "2"}], "m", "\[InvisibleSpace]", "E"}]]}], 
   TraditionalForm]],ExpressionUUID->"162ff303-9dea-46a1-a7d3-68d10608be83"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "=", 
    SqrtBox[
     RowBox[{"2", 
      RowBox[{"m", "(", 
       RowBox[{"E", "-", 
        SubscriptBox["V", "0"]}], ")"}]}]]}], TraditionalForm]],
  ExpressionUUID->"7ec8c91d-6120-4f31-af56-eac80fd7bf1f"],
 ".  Matching wavefunctions and derivatives at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "=", "1"}], TraditionalForm]],ExpressionUUID->
  "1396f65e-f2d9-4bc4-b7ac-1bae3a1b4fe9"],
 " gives\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Piecewise]", GridBox[{
      {
       RowBox[{
        RowBox[{"j", "(", "q", ")"}], "=", 
        RowBox[{"y", "\[InvisibleSpace]", 
         RowBox[{"H", "(", 
          RowBox[{"i", "\[InvisibleSpace]", "\[Lambda]"}], ")"}]}]}]},
      {
       RowBox[{
        RowBox[{"q", "\[InvisibleSpace]", 
         RowBox[{"j", "'"}], 
         RowBox[{"(", "q", ")"}]}], "=", 
        RowBox[{
        "i", "\[InvisibleSpace]", "\[Lambda]", "\[InvisibleSpace]", "y", 
         "\[InvisibleSpace]", 
         RowBox[{"H", "'"}], 
         RowBox[{"(", 
          RowBox[{"i", "\[InvisibleSpace]", "\[Lambda]"}], ")"}]}]}]}
     }]}], TraditionalForm]],ExpressionUUID->
  "c132285d-2960-4377-924a-52b096ee5c23"],
 "\nTherefore\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"i", "\[InvisibleSpace]", "\[Lambda]", "\[InvisibleSpace]", 
      RowBox[{"H", "'"}], 
      RowBox[{"(", 
       RowBox[{"i", "\[InvisibleSpace]", "\[Lambda]"}], ")"}]}], 
     RowBox[{"H", "(", 
      RowBox[{"i", "\[InvisibleSpace]", "\[Lambda]"}], ")"}]], 
    "\[InvisibleSpace]", "=", 
    FractionBox[
     RowBox[{"q", "\[InvisibleSpace]", 
      RowBox[{"j", "'"}], 
      RowBox[{"(", "q", ")"}]}], 
     RowBox[{"j", "(", "q", ")"}]]}], TraditionalForm]],ExpressionUUID->
  "d011e366-1689-4ab2-a2aa-c030ba5039e7"],
 ".\nNow, for a bound state, \[Lambda] must be positive.  Therefore the LHS \
of the equation ... is constrained in a \t\n"
}], "Text",
 CellChangeTimes->{
  3.485202631223093*^9},ExpressionUUID->"9847caa5-345d-499b-85e6-\
6d815c574991"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "r", ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {
        RowBox[{"r", "<", "1"}], 
        RowBox[{"b", "\[InvisibleSpace] ", 
         FractionBox[
          RowBox[{"sin", " ", "q", "\[InvisibleSpace]", "r"}], 
          RowBox[{"q", "\[InvisibleSpace]", "r"}]]}]},
       {
        RowBox[{"r", ">", "1"}], 
        FractionBox[
         RowBox[{"exp", " ", "-", 
          RowBox[{"\[Lambda]", "\[InvisibleSpace]", "r"}]}], 
         RowBox[{"\[Lambda]", "\[InvisibleSpace]", "r"}]]}
      }]}]}], TraditionalForm]],ExpressionUUID->
  "13f4e352-79b0-411b-8c22-30ab16873359"],
 "\nor to make it even simpler, simply factor out constants and write\n"
}], "Text",
