Cell[TextData[{
 "Consider a scalar, non-relativistic, quantum mechanical particle in a 3D \
continuum, with a spherically symmetric potential",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"V", "(", "r", ")"}]}], TraditionalForm]],ExpressionUUID->
  "52e7e207-b9c4-468a-a39a-5e4415a3c2b6"],
 ".  The Schr\[ODoubleDot]dinger equation is\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", 
        RowBox[{"2", "m"}]]}], 
      RowBox[{
       SuperscriptBox["\[Del]", "2"], "\[Psi]"}]}], "+", 
     RowBox[{
      RowBox[{"V", "(", "r", ")"}], "\[Psi]"}]}], "=", 
    RowBox[{"E", "\[InvisibleSpace]", "\[Psi]"}]}], TraditionalForm]],
  ExpressionUUID->"937c2534-1b80-409b-ba9d-600d4776be7a"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Therefore]", " ", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        SuperscriptBox["\[Del]", "2"], "\[Psi]"}]}], "+", 
      RowBox[{
       SuperscriptBox["k", "2"], "\[Psi]"}]}]}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"6aad89ea-0a0f-4b8a-8f39-97087a8fe958"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["k", "2"], "(", "r", ")"}], "=", 
    RowBox[{"2", 
     RowBox[{"m", "[", 
      RowBox[{"E", "-", 
       RowBox[{"V", "(", "r", ")"}]}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"ea46b406-ae49-42ab-bad3-1113c83c770a"],
 ".  Separation of variables in spherical polars gives\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Psi]", "(", 
     RowBox[{"r", ",", "\[Theta]", ",", "\[Phi]"}], ")"}], "=", 
    RowBox[{
     SubscriptBox[
      StyleBox["\[Sum]",
       FontSize->18], 
      RowBox[{"l", "\[NegativeMediumSpace]", "m"}]], 
     RowBox[{
      SubscriptBox["\[Psi]", 
       RowBox[{"l", "\[NegativeMediumSpace]", "m"}]], 
      RowBox[{"R", "(", "r", ")"}], 
      RowBox[{
       SubscriptBox["Y", 
        RowBox[{"l", "\[NegativeMediumSpace]", "m"}]], "(", 
       RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"a5c7d1dc-c442-4c26-a3ce-07e1e4e33bf6"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Y", "lm"], "(", 
    RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}], TraditionalForm]],
  ExpressionUUID->"eb00b02e-199e-4ba5-bfd4-8502c6429a9b"],
 " are spherical harmonic functions, and the radial wavefunction ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "(", "r", ")"}], TraditionalForm]],ExpressionUUID->
  "2e8732c2-8114-4df5-a312-068d26593001"],
 " satisfies the radial equation\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[PartialD]", "r"], "2"], 
      RowBox[{"(", 
       RowBox[{"r", "\[InvisibleSpace]", "R"}], ")"}]}], "+", 
     RowBox[{
      RowBox[{"[", 
       RowBox[{
        RowBox[{
         SuperscriptBox["k", "2"], "(", "r", ")"}], "-", 
        FractionBox[
         RowBox[{"l", "(", 
          RowBox[{"l", "+", "1"}], ")"}], 
         SuperscriptBox["r", "2"]]}], "]"}], "r", "\[InvisibleSpace]", 
      "R"}]}], "=", "0"}], TraditionalForm]],ExpressionUUID->
  "e696f3c4-c1b3-41e9-9e59-690520088969"],
 "."
}], "Text",
