Cell["\<\
ContourPlot is a simple way to generate a plot of bound state energies as a \
function of well depth:\
\>", "Text",
 CellChangeTimes->{{3.485303343681532*^9, 3.485303346807366*^9}, {
  3.485355737181048*^9, 
  3.485355751643627*^9}},ExpressionUUID->"ff7aa074-af57-4b9d-a5ab-\
1e439acb506e"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "\[CurlyEpsilon]", ",", "V", ",", "l", ",", "\[Lambda]", ",", "q", ",", 
    "residual"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"residual", "[", 
    RowBox[{"l_", ",", "V_", ",", "\[CurlyEpsilon]_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{
      RowBox[{"q", "*", 
       FractionBox[
        RowBox[{"jprime", "[", 
         RowBox[{"l", ",", "q"}], "]"}], 
        RowBox[{"\[Lambda]", " "}]]}], "-", 
      FractionBox[
       RowBox[{" ", 
        RowBox[{"I", "*", 
         RowBox[{"Hprime", "[", 
          RowBox[{"l", ",", 
           RowBox[{"I", " ", "\[Lambda]"}]}], "]"}], "*", 
         RowBox[{"j", "[", 
          RowBox[{"l", ",", "q"}], "]"}]}]}], 
       RowBox[{"H", "[", 
        RowBox[{"l", ",", 
         RowBox[{"I", " ", "\[Lambda]"}]}], "]"}]]}], "]"}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Lambda]", "\[Rule]", 
       SqrtBox[
        RowBox[{
         RowBox[{"-", "2"}], "\[CurlyEpsilon]"}]]}], ",", 
      RowBox[{"q", "\[Rule]", 
       SqrtBox[
        RowBox[{"2", 
         RowBox[{"(", 
          RowBox[{"\[CurlyEpsilon]", "-", "V"}], ")"}]}]]}]}], "}"}]}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.48534018972456*^9, 3.4853402304967613`*^9}, {
  3.485340285057535*^9, 
  3.48534029231066*^9}},ExpressionUUID->"aa565892-01e8-4acf-8491-\
c978cf431f19"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ContourPlot", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"residual", "[", 
    RowBox[{"0", ",", "V", ",", "\[CurlyEpsilon]"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"V", ",", 
     RowBox[{"-", "100."}], ",", 
     RowBox[{"-", "0.01"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[CurlyEpsilon]", ",", 
     RowBox[{"-", "99."}], ",", 
     RowBox[{"-", "0.001"}]}], "}"}], ",", 
   RowBox[{"Contours", "\[Rule]", 
    RowBox[{"{", "0", "}"}]}], ",", "\[IndentingNewLine]", 
   RowBox[{"PlotRangePadding", "\[Rule]", "0"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "256"}], ",", 
   RowBox[{"AspectRatio", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"ContourShading", "\[Rule]", "None"}], ",", 
   RowBox[{"ContourStyle", "\[Rule]", 
    RowBox[{"Directive", "@", 
     RowBox[{"{", 
      RowBox[{"Thick", ",", "Red"}], "}"}]}]}], ",", "\[IndentingNewLine]", 
   RowBox[{"PlotRange", "\[Rule]", "All"}], ",", "\[IndentingNewLine]", 
   RowBox[{"FrameLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\"\<Well potential \!\(\*StyleBox[\"V\",FontSlant->\"Italic\"]\)\>\"", 
      ",", "\"\<Bound state energy \
\!\(\*StyleBox[\"E\",FontSlant->\"Italic\"]\)\>\""}], "}"}]}], ",", 
   RowBox[{"RotateLabel", "\[Rule]", "True"}], ",", "\[IndentingNewLine]", 
