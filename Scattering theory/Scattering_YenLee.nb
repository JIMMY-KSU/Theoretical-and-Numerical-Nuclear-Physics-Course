Cell[TextData[{
 "The optical sum rule gives the total scattering cross-section ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Sigma]", "total"], TraditionalForm]],ExpressionUUID->
  "299ea2e9-00fa-4fb6-9119-7d04116ab528"],
 " as a sum of partial-wave cross-sections ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Sigma]", "l"], TraditionalForm]],ExpressionUUID->
  "0f104272-3448-48fb-be20-04146fa6f4b5"],
 ", which are basically ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["sin", "2"], 
    SubscriptBox["\[Delta]", "l"]}], TraditionalForm]],ExpressionUUID->
  "156634cf-2bf8-4bf5-90c5-f5bc8e630cb5"],
